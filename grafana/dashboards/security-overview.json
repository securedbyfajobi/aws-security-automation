{
  "dashboard": {
    "id": null,
    "title": "AWS Security Automation - Overview",
    "description": "Main dashboard for AWS security automation monitoring",
    "tags": ["aws", "security", "guardduty", "securityhub"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Security Findings Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(guardduty_findings_total)",
            "legendFormat": "Total Findings",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Critical/High Severity Findings",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(guardduty_findings_total{severity=~\"CRITICAL|HIGH\"})",
            "legendFormat": "Critical/High",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "orange", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Compliance Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(sum(securityhub_compliance_passed_total) / (sum(securityhub_compliance_passed_total) + sum(securityhub_compliance_failed_total))) * 100",
            "legendFormat": "Compliance %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 4,
        "title": "Processing Queue Length",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "redis_list_length{key=\"security_findings_queue\"}",
            "legendFormat": "Queue Length",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Findings by Severity Over Time",
        "type": "timeseries",
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(guardduty_findings_total{severity=\"CRITICAL\"}[5m])",
            "legendFormat": "Critical",
            "refId": "A"
          },
          {
            "expr": "rate(guardduty_findings_total{severity=\"HIGH\"}[5m])",
            "legendFormat": "High",
            "refId": "B"
          },
          {
            "expr": "rate(guardduty_findings_total{severity=\"MEDIUM\"}[5m])",
            "legendFormat": "Medium",
            "refId": "C"
          },
          {
            "expr": "rate(guardduty_findings_total{severity=\"LOW\"}[5m])",
            "legendFormat": "Low",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Top Finding Types",
        "type": "piechart",
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "topk(5, sum by (finding_type) (guardduty_findings_total))",
            "legendFormat": "{{finding_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            }
          }
        },
        "options": {
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "displayLabels": ["name"]
        }
      },
      {
        "id": 7,
        "title": "Remediation Success Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "rate(security_remediation_success_total[5m]) / (rate(security_remediation_success_total[5m]) + rate(security_remediation_failed_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 8,
        "title": "Processing Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 18},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(finding_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(finding_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "50th Percentile",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(process_resident_memory_bytes / 1024 / 1024)",
            "legendFormat": "Memory Usage MB",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(up{job=\"security-automation\"}[1m])",
          "iconColor": "green",
          "titleFormat": "Deployment",
          "textFormat": "Service restarted"
        }
      ]
    }
  }
}